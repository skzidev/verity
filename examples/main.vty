import "std/fs" as fs;

proc add(int a, int b) returns int {
    return a + b;
}

proc readFile(string path) returns string throws FileNotFoundError {
	if(path != "./config.txt"){
		print("somebody tweaked the code");
	}
	return propogate fs.readAsText(path);
}

int x = 5;
mut int y = 10;
y = x / y;

print(add(x, y));
print(readFile("./config.txt") handle FileNotFoundError as e => print(e));